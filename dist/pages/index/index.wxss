}
.page_home .more_guess .body .content {
  flex: 1;
  display: flex;
  flex-direction: column;
}
.page_home .more_guess .body .num {
  font-size: 22rpx;
  color: #a4a5a5;
}
.page_home .more_guess .body .content_inner {
  display: flex;
  align-items: center;
  flex: 1;
}
.page_home .more_guess .num_wrap {
  display: flex;
  justify-content: flex-end;
}

