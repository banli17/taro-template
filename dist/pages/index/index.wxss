  font-size: 28rpx;
  color: rgba(59, 63, 65, 0.85);
  line-height: 46rpx;
}
.page_home .more_guess .item {
  display: flex;
  flex-direction: row;
  background: #ffffff;
  box-shadow: 2rpx 6rpx 8rpx 0 #e9e9e9;
  border-radius: 14rpx;
  border: 2rpx solid rgba(238, 238, 238, 0.45);
  padding: 18rpx 12rpx;
  margin-bottom: 24rpx;
}
.page_home .more_guess .body .icon {
  width: 178rpx;
  height: 138rpx;
  background: #eee;
  margin-right: 10rpx;
}
.page_home .more_guess .body .title {
  font-size: 28rpx;
