}
.page_home .yunshi_today_desc {
  background: #ffffff;
  box-shadow: 2rpx 4rpx 8rpx 0 #e7e7e7;
  border-radius: 14rpx;
  border: 2rpx solid rgba(238, 238, 238, 0.43);
}
.page_home .yunshi_today_desc .title {
  width: 191rpx;
  height: 58rpx;
}
.page_home .yunshi_today_desc .body {
